    cell (not_gate) {
        area : "0.133120";
        pin (O) {
            direction : "output";
            function : "!I";
	        timing () {
	            related_pin : "I";
	            timing_type: combinational;
	            timing_sense : negative_unate;
	            cell_fall(scalar) { values ("2.0"); }
	            cell_rise(scalar) { values ("2.0"); }
	            fall_transition(scalar) { values ("0.01"); }
	            rise_transition(scalar) { values ("0.01"); }
